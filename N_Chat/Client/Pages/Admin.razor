@inject Services.IUserService IUserService
@inject Services.IChatService IChatService
@inject NavigationManager NavManeger
@page "/Admin"

<AuthorizeView Roles="Admin">
    <Authorized>
        <h3>welcome to admin</h3>
    </Authorized>
</AuthorizeView>
<AuthorizeView>
    <NotAuthorized>
        <h3>L</h3>
    </NotAuthorized>
</AuthorizeView>


@code {
    ICollection<UserModel>? UserList { get; set; }
    ICollection<ChatModel>? ChatList { get; set; }

    public async Task OnIntializedAsync()
    {
        UserList = await IUserService.GetAllUsers();
        ChatList = await IChatService.GetChats();
    }
}
